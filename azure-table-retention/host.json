{
  "version": "2.0",
  "CORS": "*",
  "tracing": { "consoleLevel": "information" },
  "healthMonitor": { "enabled": "warning" },
  "extensions": {

    "durableTask": {
      "extendedSessionsEnabled": true,
      "extendedSessionIdleTimeoutInSeconds": 120,
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage"
      }
    }

  },
  "logging": {
    "logLevel": {
      "default": "warning"
    },
    "applicationInsights": {
      "samplingExcludedTypes": "",
      "samplingSettings": {
        "isEnabled": false
      }
    }
  }
}