@model IEnumerable<com.ataxlab.azure.table.retention.models.models.auth.ApplianceSessionContext>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SelectedSubscriptionId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SelectedStorageAccountId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserOid)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SelectedSubscriptionId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SelectedStorageAccountId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserOid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
